<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/teachers_side"
    app:startDestination="@id/homePageTeachersSide">

    <activity
        android:id="@+id/navBarActivity"
        android:name="com.example.unacademy.Activities.NavBarActivity"
        android:label="activity_nav_bar"
        tools:layout="@layout/activity_nav_bar" />
    <fragment
        android:id="@+id/homePageTeachersSide"
        android:name="com.example.unacademy.Ui.TeachersSide.HomePageTeachersSide"
        android:label="fragment_home_page_teachers_side"
        tools:layout="@layout/fragment_home_page_teachers_side" >
        <action
            android:id="@+id/action_homePageTeachersSide_to_createYourSeries2"
            app:destination="@id/createYourSeries" />
    </fragment>
    <fragment
        android:id="@+id/profileTeachersSide"
        android:name="com.example.unacademy.Ui.TeachersSide.ProfileTeachersSide"
        android:label="fragment_profile_teachers_side"
        tools:layout="@layout/fragment_profile_teachers_side" >
        <action
            android:id="@+id/action_profileTeachersSide_to_makeAnnouncement2"
            app:destination="@id/makeAnnouncement2" />
        <action
            android:id="@+id/action_profileTeachersSide_to_change_teachers_profile"
            app:destination="@id/change_teachers_profile" />
    </fragment>
    <fragment
        android:id="@+id/createYourSeries"
        android:name="com.example.unacademy.Ui.TeachersSide.CreateYourSeries"
        android:label="CreateYourSeries" />
    <fragment
        android:id="@+id/makeAnnouncement2"
        android:name="com.example.unacademy.Ui.TeachersSide.MakeAnnouncement"
        android:label="fragment_make_announcement"
        tools:layout="@layout/fragment_make_announcement" />
    <fragment
        android:id="@+id/change_teachers_profile"
        android:name="com.example.unacademy.Ui.TeachersSide.change_teachers_profile"
        android:label="change_teachers_profile" />
</navigation>